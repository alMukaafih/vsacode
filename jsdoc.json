{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["."],
        "exclude": ["node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "defaults": {
            "useLongnameInNav": true
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./js-docs",
        "recurse": true,
        "template": "node_modules/docdash"
    },
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu":{
            "Github repo": {
                "href":"https://github.com/your-github-username/pet-adoption-center-sdk",
                "target":"_blank",
                "class":"menu-item",
                "id":"repository"
            }
        }
    }
}
